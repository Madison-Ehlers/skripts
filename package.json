{
  "name": "skripts",
  "version": "0.0.1",
  "description": "CLI for project scripts and configuration.",
  "homepage": "https://github.com/therockstorm/skripts#readme",
  "bugs": {
    "url": "https://github.com/therockstorm/skripts/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Rocky Warren",
    "url": "https://www.rockywarren.com"
  },
  "files": [
    "dist",
    "config.js",
    "tslint.js"
  ],
  "main": "dist/index.js",
  "bin": {
    "skripts": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/therockstorm/skripts.git"
  },
  "scripts": {
    "build": "tsc --outDir dist && cp -r src/config dist/config",
    "clean": "npm run script -- clean dist",
    "deploy": "npm run script -- deploy",
    "format": "npm run script -- format",
    "lint": "npm run script -- lint",
    "push": "npm run clean && npm run build && npm publish",
    "script": "npm run build && node dist/index.js",
    "test": "npm run script -- jest",
    "help": "npm run build && node dist/index.js --help",
    "watch": "npm run script -- watch"
  },
  "dependencies": {
    "@therockstorm/utils": "^3.0.3",
    "@types/aws-lambda": "^8.10.19",
    "@types/jest": "^24.0.8",
    "@types/node": "^11.9.5",
    "commander": "^2.19.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "serverless": "^1.37.1",
    "serverless-webpack": "^5.2.0",
    "source-map-support": "^0.5.10",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3",
    "webpack": "^4.29.5",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run script -- pre-commit"
    }
  },
  "devDependencies": {
    "husky": "^1.3.1"
  }
}
